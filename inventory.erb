[OSEv3:children]
masters
nodes
etcd

[OSEv3:vars]
ansible_ssh_user=root
openshift_deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v3.7.0
openshift_image_tag=v3.7.0-rc.0
openshift_ansible_branch=release-3.7
openshift_enable_excluders=false
openshift_public_hostname=openshift.console.com
openshift_metrics_install_metrics=true
openshift_hosted_metrics_deploy=true
openshift_hosted_metrics_selector='metrics=enabled'
openshift_hosted_logging_deploy=true
openshift_disable_check=docker_image_availability,memory_availability,disk_availability,docker_storage,docker_storage_driver

[masters]
openshift.console.com openshift_schedulable=true

[etcd]
openshift.console.com

[nodes]
openshift.console.com openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
